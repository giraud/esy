{
  "versions": {
    "0.9.1": {
      "version": "0.9.1", 
      "opam": {
        "url": "https://github.com/mfp/obigstore/archive/obigstore-0.9.1.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "omake -j9", 
          "omake install prefix=$cur__install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "obigstore_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "obigstore_version": {
            "scope": "global", 
            "val": "0.9.1"
          }, 
          "obigstore_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/lwt": "< 2.4.7", 
        "@opam/cryptokit": "*", 
        "@opam/omake": "*", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/extprot": "*", 
        "@opam/batteries": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/leveldb": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/obigstore"
    }
  }
}