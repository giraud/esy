{
  "versions": {
    "0.9.0": {
      "version": "0.9.0", 
      "opam": {
        "url": "https://github.com/nojb/ocaml-maildir/archive/v0.9.0.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "oasis setup", 
          "./configure --prefix $cur__install", 
          "make build", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "maildir_version": {
            "scope": "global", 
            "val": "0.9.0"
          }, 
          "maildir_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "maildir_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/oasis": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/maildir"
    }
  }
}