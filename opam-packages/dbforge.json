{
  "versions": {
    "2.0.1": {
      "version": "2.0.1", 
      "opam": {
        "url": "http://zoggy.github.com/dbforge/dbforge-2.0.1.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure --prefix $cur__install", 
          "make all", 
          "make install-lib", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "dbforge_version": {
            "scope": "global", 
            "val": "2.0.1"
          }, 
          "dbforge_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "dbforge_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/config-file": ">= 1.1.0", 
        "@opam/mysql": "*", 
        "@opam/conf-glade": "= 2.0.0", 
        "@opam/xml-light": "*", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/lablgtk": ">= 2.16.0", 
        "@opam/lablgtk-extras": ">= 1.2.0", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/dbforge"
    }, 
