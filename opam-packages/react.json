{
  "versions": {
    "1.2.0": {
      "version": "1.2.0", 
      "opam": {
        "url": "http://erratique.ch/software/react/releases/react-1.2.0.tbz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml pkg/git.ml", 
          "ocaml pkg/build.ml native=true native-dynlink=true", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "react_version": {
            "scope": "global", 
            "val": "1.2.0"
          }, 
          "react_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "react_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/ocamlbuild": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/react"
    }
  }
}