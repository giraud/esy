{
  "versions": {
    "1.0.0": {
      "version": "1.0.0", 
      "opam": {
        "url": "http://forge.ocamlcore.org/frs/download.php/990/mascot-1.0.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure", 
          "make all", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "mascot_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "mascot_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "mascot_version": {
            "scope": "global", 
            "val": "1.0.0"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/camlp4": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.0.0 < 4.02.0"
      }, 
      "name": "@opam/mascot"
    }
  }
}