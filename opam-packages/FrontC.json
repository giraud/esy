{
  "versions": {
    "3.4.0": {
      "version": "3.4.0", 
      "opam": {
        "url": "http://www.irit.fr/recherches/ARCHI/MARCH/frontc/Frontc-3.4.tgz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make install PREFIX=$cur__lib OCAML_SITE=$cur__lib", 
          "cp META $cur__lib/FrontC", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "FrontC_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "FrontC_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "FrontC_version": {
            "scope": "global", 
            "val": "3.4.0"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/FrontC"
    }
  }
}