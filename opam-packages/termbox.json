{
  "versions": {
    "0.1.0": {
      "version": "0.1.0", 
      "opam": {
        "url": "https://github.com/pacemkr/ocaml-termbox/tarball/v0.1.0"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "termbox_version": {
            "scope": "global", 
            "val": "0.1.0"
          }, 
          "termbox_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "termbox_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/termbox"
    }
  }
}