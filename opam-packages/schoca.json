{
  "versions": {
    "0.2.3": {
      "version": "0.2.3", 
      "opam": {
        "url": "http://downloads.sourceforge.net/project/chesslib/Schoca/schoca-0.2.3/schoca-0.2.3.tar.bz2"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make", 
          "make install DESTPREF=$cur__install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "schoca_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "schoca_version": {
            "scope": "global", 
            "val": "0.2.3"
          }, 
          "schoca_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/camlp4": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/schoca"
    }
  }
}