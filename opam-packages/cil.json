{
  "versions": {
    "1.7.3": {
      "version": "1.7.3", 
      "opam": {
        "url": "http://downloads.sourceforge.net/project/cil/cil/cil-1.7.3.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "env FORCE_PERL_PREFIX=1 ./configure --prefix $cur__install", 
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "cil_version": {
            "scope": "global", 
            "val": "1.7.3"
          }, 
          "cil_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "cil_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/cil"
    }, 
