{
  "versions": {
    "1.0.0": {
      "version": "1.0.0", 
      "opam": {
        "url": "http://forge.ocamlcore.org/frs/download.php/330/ocamlagrep-1.0.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make", 
          "make find-install", 
          "make find-install LIB_EXT=dll", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "agrep_version": {
            "scope": "global", 
            "val": "1.0.0"
          }, 
          "agrep_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "agrep_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/agrep"
    }
  }
}