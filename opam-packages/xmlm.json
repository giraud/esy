        "url": "http://erratique.ch/software/xmlm/releases/xmlm-1.1.1.tbz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "xmlm_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "xmlm_version": {
            "scope": "global", 
            "val": "1.1.1"
          }, 
          "xmlm_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 3.10.0"
      }, 
      "name": "@opam/xmlm"
    }, 
