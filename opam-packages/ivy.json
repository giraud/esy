{
  "versions": {
    "1.3.1": {
      "version": "1.3.1", 
      "opam": {
        "url": "http://www.eei.cena.fr/products/ivy/download/packages/ivy-ocaml_1.3.1.tar.xz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "ivy_version": {
            "scope": "global", 
            "val": "1.3.1"
          }, 
          "ivy_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "ivy_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/conf-tk": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/ivy"
    }, 
    "1.2.2": {
      "version": "1.2.2", 
      "opam": {
        "url": "http://www.eei.cena.fr/products/ivy/download/packages/ivy-ocaml_1.2-2.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make", 
          "make install COMPAT_SYMLINK_CREATE=no", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "ivy_version": {
            "scope": "global", 
            "val": "1.2.2"
          }, 
          "ivy_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "ivy_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/conf-tcl": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/ivy"
    }
  }
}