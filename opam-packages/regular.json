{
  "versions": {
    "1.1.0": {
      "version": "1.1.0", 
      "opam": {
        "url": "https://github.com/BinaryAnalysisPlatform/bap/archive/v1.1.0.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure --prefix=$cur__install --mandir=$cur__man --enable-regular", 
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "regular_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "regular_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "regular_version": {
            "scope": "global", 
            "val": "1.1.0"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/core_kernel": ">= 113.24.00", 
        "@opam/oasis": "= 0.4.7", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/ppx_jane": ">= 113.24.01"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.02.3 < 4.04.0"
      }, 
      "name": "@opam/regular"
    }, 
