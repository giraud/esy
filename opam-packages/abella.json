{
  "versions": {
    "2.0.4": {
      "version": "2.0.4", 
      "opam": {
        "url": "https://codeload.github.com/abella-prover/abella/tar.gz/v2.0.4"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "abella_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "abella_version": {
            "scope": "global", 
            "val": "2.0.4"
          }, 
          "abella_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.02.3"
      }, 
      "name": "@opam/abella"
    }
  }
}