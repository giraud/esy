{
  "versions": {
    "4.0.0": {
      "version": "4.0.0", 
      "opam": {
        "url": "https://github.com/darioteixeira/bookaml/archive/v4.0.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure --prefix $cur__install --docdir $cur__doc/bookaml --${ocsigenserver_enable:-disable}-ocsigen --${ocamlnet_enable:-disable}-ocamlnet", 
          "make", 
          "make doc", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "bookaml_version": {
            "scope": "global", 
            "val": "4.0.0"
          }, 
          "bookaml_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "bookaml_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/ocsigenserver": "*", 
        "@opam/lwt": "*", 
        "@opam/ocamlnet": ">= 4.0.0", 
        "@opam/cryptokit": "*", 
        "@opam/camlp4": "*", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/tyxml": "*", 
        "@opam/batteries": "*", 
        "@opam/ppx_sexp_conv": "*", 
        "@opam/calendar": "*", 
        "@opam/sexplib": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.01.0"
      }, 
      "name": "@opam/bookaml"
    }, 
