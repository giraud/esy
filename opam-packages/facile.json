{
  "versions": {
    "1.1.0": {
      "version": "1.1.0", 
      "opam": {
        "url": "http://opti.recherche.enac.fr/facile/distrib/facile-1.1.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure -faciledir $cur__lib/facile", 
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "facile_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "facile_version": {
            "scope": "global", 
            "val": "1.1.0"
          }, 
          "facile_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/facile"
    }
  }
}