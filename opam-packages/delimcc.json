{
  "versions": {
    "0.0.0": {
      "version": "0.0.0", 
      "opam": {
        "url": "http://okmij.org/ftp/continuations/caml-shift.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make all", 
          "make opt", 
          "make findlib-install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "delimcc_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "delimcc_version": {
            "scope": "global", 
            "val": "0.0.0"
          }, 
          "delimcc_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": "< 4.03.0 >= 4.04.0"
      }, 
      "name": "@opam/delimcc"
    }
  }
}