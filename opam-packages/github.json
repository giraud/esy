{
  "versions": {
    "2.2.0": {
      "version": "2.2.0", 
      "opam": {
        "url": "https://github.com/mirage/ocaml-github/archive/v2.2.0.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml setup.ml -configure --${base_unix_enable:-disable}-unix --${js_of_ocaml_enable:-disable}-js --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "github_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "github_version": {
            "scope": "global", 
            "val": "2.2.0"
          }, 
          "github_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/lwt": ">= 2.4.4", 
        "@opam/uri": ">= 1.9.0", 
        "@opam/cmdliner": ">= 0.9.8", 
        "@opam/yojson": ">= 1.2.0", 
        "@opam/cohttp": ">= 0.17.0", 
        "@opam/lambda-term": "*", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/stringext": "*", 
        "@opam/tls": "*", 
        "@opam/js_of_ocaml": "*", 
        "@opam/atdgen": ">= 1.10.0", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.02.3"
      }, 
      "name": "@opam/github"
    }
  }
}