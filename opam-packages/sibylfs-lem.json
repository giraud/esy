{
  "versions": {
    "0.4.0": {
      "version": "0.4.0", 
      "opam": {
        "url": "https://bitbucket.org/dsheets/lem/get/0.4.0.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make -e", 
          "make ocaml-libs", 
          "make coq-libs", 
          "sh -ex ./generate_install_manifest.sh", 
          "sh -ex ./ocamlfind_install.sh", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "sibylfs_lem_version": {
            "scope": "global", 
            "val": "0.4.0"
          }, 
          "sibylfs_lem_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "sibylfs_lem_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/coq": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/sibylfs-lem"
    }
  }
}