{
  "versions": {
    "0.0.1": {
      "version": "0.0.1", 
      "opam": {
        "url": "http://forge.ocamlcore.org/frs/download.php/379/ocamlify-0.0.1.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "ocamlify_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "ocamlify_version": {
            "scope": "global", 
            "val": "0.0.1"
          }, 
          "ocamlify_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/ocamlify"
    }
  }
}