{
  "versions": {
    "1.0.0": {
      "version": "1.0.0", 
      "opam": {
        "url": null
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "sh ./detect_program.sh ./conf-bmake.config bmake", 
          "sh ./detect_program.sh ./conf-bmake.config make", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "conf_bmake_version": {
            "scope": "global", 
            "val": "1.0.0"
          }, 
          "conf_bmake_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "conf_bmake_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/conf-bmake"
    }
  }
}