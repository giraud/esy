{
  "versions": {
    "1.0.0": {
      "version": "1.0.0", 
      "opam": {
        "url": "https://forge.ocamlcore.org/frs/download.php/728/camldbm-1.0.tgz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "mkdir -p $cur__lib/dbm", 
          "./configure", 
          "make all", 
          "make test", 
          "make install STUBLIBDIR=$cur__lib/stublibs LIBDIR=$cur__lib/dbm", 
          "cp META $cur__lib/dbm", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "dbm_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "dbm_version": {
            "scope": "global", 
            "val": "1.0.0"
          }, 
          "dbm_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/dbm"
    }
  }
}