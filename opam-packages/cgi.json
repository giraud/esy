{
  "versions": {
    "0.8.0": {
      "version": "0.8.0", 
      "opam": {
        "url": "https://www.lri.fr/~filliatr/ftp/ocaml/cgi/cgi-0.8.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure", 
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "cgi_version": {
            "scope": "global", 
            "val": "0.8.0"
          }, 
          "cgi_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "cgi_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/cgi"
    }
  }
}