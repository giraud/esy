{
  "versions": {
    "0.2.3": {
      "version": "0.2.3", 
      "opam": {
        "url": "http://raevnos.pennmush.org/code/ocaml-xml-rpc/ocaml-xml-rpc-0.2.3.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make", 
          "make opt", 
          "make install BIN_DIR=$cur__bin", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "ocaml_xml_rpc_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "ocaml_xml_rpc_version": {
            "scope": "global", 
            "val": "0.2.3"
          }, 
          "ocaml_xml_rpc_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/ocamlnet": ">= 3.6.0 < 4.0.0", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/pxp": "*", 
        "@opam/annexlib": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/ocaml-xml-rpc"
    }
  }
}