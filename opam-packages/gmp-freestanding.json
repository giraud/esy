{
  "versions": {
    "6.0.0": {
      "version": "6.0.0", 
      "opam": {
        "url": "https://gmplib.org/download/gmp/gmp-6.0.0a.tar.xz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "sh -ex ./mirage-build.sh", 
          "sh -ex ./mirage-install.sh", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "gmp_freestanding_version": {
            "scope": "global", 
            "val": "6.0.0"
          }, 
          "gmp_freestanding_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "gmp_freestanding_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocaml-freestanding": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/gmp-freestanding"
    }
  }
}