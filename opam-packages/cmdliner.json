{
  "versions": {
    "0.9.8": {
      "version": "0.9.8", 
      "opam": {
        "url": "http://erratique.ch/software/cmdliner/releases/cmdliner-0.9.8.tbz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml pkg/git.ml", 
          "ocaml pkg/build.ml native=true native-dynlink=true", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "cmdliner_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "cmdliner_version": {
            "scope": "global", 
            "val": "0.9.8"
          }, 
          "cmdliner_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 3.12.0"
      }, 
      "name": "@opam/cmdliner"
    }
  }
}