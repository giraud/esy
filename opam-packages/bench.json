{
  "versions": {
    "1.3.0": {
      "version": "1.3.0", 
      "opam": {
        "url": "http://oasis.ocamlcore.org/dev/dist/bench/1.3/bench-1.3.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "rm setup.ml", 
          "oasis setup", 
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "bench_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "bench_version": {
            "scope": "global", 
            "val": "1.3.0"
          }, 
          "bench_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/oasis": ">= 0.3.0 < 0.4.7", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/bench"
    }
  }
}