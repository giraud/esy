{
  "versions": {
    "0.1.0": {
      "version": "0.1.0", 
      "opam": {
        "url": "https://github.com/ocsigen/ojquery/archive/0.1.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure --prefix $cur__install", 
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "ojquery_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "ojquery_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "ojquery_version": {
            "scope": "global", 
            "val": "0.1.0"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/lwt": "*", 
        "@opam/react": "*", 
        "@opam/js_of_ocaml": "< 2.4.0", 
        "substs": "esy-ocaml-test/substs", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/ojquery"
    }
  }
}