{
  "versions": {
    "0.6.2": {
      "version": "0.6.2", 
      "opam": {
        "url": "http://download.camlcity.org/download/plasma-0.6.2.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure -bindir $cur__bin -sharedir $cur__lib/plasma/share", 
          "omake", 
          "omake install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "plasma_version": {
            "scope": "global", 
            "val": "0.6.2"
          }, 
          "plasma_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "plasma_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/ocamlgraph": "= 1.8.2", 
        "@opam/ocamlnet": "*", 
        "@opam/cryptokit": "*", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/omake": "*", 
        "@opam/pcre": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/xstrp4": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": "< 4.01.0"
      }, 
      "name": "@opam/plasma"
    }, 
