{
  "versions": {
    "1.05.0": {
      "version": "1.05.0", 
      "opam": {
        "url": "https://forge.ocamlcore.org/frs/download.php/1254/lablgl-1.05.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "cp Makefile.config.ex Makefile.config", 
          "cp Makefile.config.osx Makefile.config", 
          "mkdir -p $cur__bin", 
          "make INSTALLDIR=$cur__lib/lablgl glut", 
          "make glutopt", 
          "make install -C src BINDIR=$cur__bin INSTALLDIR=$cur__lib/lablgl DLLDIR=$cur__lib/stublibs", 
          "make install -C LablGlut/src BINDIR=$cur__bin INSTALLDIR=$cur__lib/lablgl DLLDIR=$cur__lib/stublibs", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "lablgl_version": {
            "scope": "global", 
            "val": "1.05.0"
          }, 
          "lablgl_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "lablgl_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/camlp4": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/lablgl"
    }, 
