{
  "versions": {
    "1.3.0": {
      "version": "1.3.0", 
      "opam": {
        "url": "https://github.com/mjambon/mjambon2016/raw/master/camlmix/camlmix-1.3.0.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make", 
          "make install PREFIX=$cur__install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "camlmix_version": {
            "scope": "global", 
            "val": "1.3.0"
          }, 
          "camlmix_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "camlmix_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/camlmix"
    }
  }
}