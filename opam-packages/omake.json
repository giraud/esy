        "build": [
          "make bootstrap PREFIX=$cur__install", 
          "make all PREFIX=$cur__install", 
          "make install PREFIX=$cur__install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "omake_version": {
            "scope": "global", 
            "val": "0.9.8.6-0.rc1"
