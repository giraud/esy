{
  "versions": {
    "2.5.2": {
      "version": "2.5.2", 
      "opam": {
        "url": "https://github.com/ocsigen/lwt/archive/2.5.2.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "(make clean || true)", 
          "mkdir -p src/unix/jobs-unix", 
          "./configure --prefix $cur__install --${conf_libev_enable:-disable}-libev --${camlp4_enable:-disable}-camlp4 --${react_enable:-disable}-react --${ssl_enable:-disable}-ssl --${base_unix_enable:-disable}-unix --${base_threads_enable:-disable}-preemptive --${lablgtk_enable:-disable}-glib --${ppx_tools_enable:-disable}-ppx", 
          "make build", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "lwt_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "lwt_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "CAML_LD_LIBRARY_PATH": {
            "scope": "global", 
            "val": "$opam_lwt__lib/lwt:$CAML_LD_LIBRARY_PATH"
          }, 
          "lwt_version": {
            "scope": "global", 
            "val": "2.5.2"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": ">= 1.5.0", 
        "@opam/ocamlbuild": "*", 
        "@opam/react": "*", 
        "@opam/camlp4": "*", 
        "@opam/conf-ncurses": "*", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/ppx_tools": "*", 
        "@opam/base-threads": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/base-unix": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.01.0"
      }, 
      "name": "@opam/lwt"
    }
  }
}