{
  "versions": {
    "1.4.0": {
      "version": "1.4.0", 
      "opam": {
        "url": "https://github.com/diml/zed/archive/1.4.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "zed_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "zed_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "zed_version": {
            "scope": "global", 
            "val": "1.4.0"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/react": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/camomile": ">= 0.8.0"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 3.12.0"
      }, 
      "name": "@opam/zed"
    }
  }
}