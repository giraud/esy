{
  "versions": {
    "0.1.0": {
      "version": "0.1.0", 
      "opam": {
        "url": "https://github.com/stedolan/linkage/archive/v0.1.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml pkg/pkg.ml build --pinned false", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "linkage_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "linkage_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "linkage_version": {
            "scope": "global", 
            "val": "0.1.0"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/topkg": ">= 0.7.4", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.03.0"
      }, 
      "name": "@opam/linkage"
    }
  }
}