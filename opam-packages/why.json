      "version": "2.32.0", 
      "opam": {
        "url": "http://why.lri.fr/download/why-2.32.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "substs opam.patch.in", 
          "autoconf", 
          "./configure --enable-verbosemake OCAMLGRAPHLIB=$cur__lib/ocamlgraph --prefix $cur__install --sbindir=$cur__lib/why/sbin --libexecdir=$cur__lib/why/libexec --sysconfdir=$cur__lib/why/etc --sharedstatedir=$cur__lib/why/com --localstatedir=$cur__lib/why/var --libdir=$cur__lib/why/lib --includedir=$cur__lib/why/include --datarootdir=$cur__lib/why/share", 
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "why_version": {
            "scope": "global", 
            "val": "2.32.0"
          }, 
          "why_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "why_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "@opam/why3": "= 0.81.0", 
        "@opam/ocamlgraph": "= 1.8.2", 
        "@opam/alt-ergo": ">= 0.95.0", 
        "substs": "esy-ocaml-test/substs", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/conf-autoconf": "*", 
        "@opam/coq": ">= 8.4.0", 
        "@opam/frama-c": "= 20120901.0.0", 
        "@opam/lablgtk": "<= 2.16.0"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/why"
