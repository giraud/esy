{
  "versions": {
    "1.30.0": {
      "version": "1.30.0", 
      "opam": {
        "url": "http://alt-ergo.ocamlpro.com/http/alt-ergo-1.30/alt-ergo-1.30.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure -prefix $cur__install", 
          "make", 
          "make install MANDIR=$cur__man", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "alt_ergo_version": {
            "scope": "global", 
            "val": "1.30.0"
          }, 
          "alt_ergo_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "alt_ergo_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocplib-simplex": "= 0.3.0", 
        "@opam/camlzip": "*", 
        "@opam/zarith": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.01.0"
      }, 
      "name": "@opam/alt-ergo"
    }
  }
}