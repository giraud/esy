{
  "versions": {
    "1.8.7": {
      "version": "1.8.7", 
      "opam": {
        "url": "http://ocamlgraph.lri.fr/download/ocamlgraph-1.8.7.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "touch ./configure", 
          "./configure", 
          "make", 
          "make install-findlib", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "ocamlgraph_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "ocamlgraph_version": {
            "scope": "global", 
            "val": "1.8.7"
          }, 
          "ocamlgraph_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/ocamlgraph"
    }
  }
}