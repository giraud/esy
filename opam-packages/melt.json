{
  "versions": {
    "1.4.0": {
      "version": "1.4.0", 
      "opam": {
        "url": "https://forge.ocamlcore.org/frs/download.php/727/melt-1.4.0.tgz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml configure.ml -INSTALLBIN $cur__bin -INSTALLLIB $cur__lib/melt -INSTALLMAN $cur__man/man1", 
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "melt_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "melt_version": {
            "scope": "global", 
            "val": "1.4.0"
          }, 
          "melt_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/cairo": "= 1.2.0", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/mlpost": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/melt"
    }
  }
}