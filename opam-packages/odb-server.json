{
  "versions": {
    "0.1.0": {
      "version": "0.1.0", 
      "opam": {
        "url": "http://forge.ocamlcore.org/frs/download.php/722/odb-server_0.1.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "substs opam.patch.in", 
          "mkdir -p $cur__bin", 
          "make", 
          "make install OCAMLLIB=$cur__lib", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "odb_server_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "odb_server_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "odb_server_version": {
            "scope": "global", 
            "val": "0.1.0"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/camlp4": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/odb-server"
    }
  }
}