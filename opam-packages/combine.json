{
  "versions": {
    "0.55.0": {
      "version": "0.55.0", 
      "opam": {
        "url": "https://github.com/backtracking/combine/archive/opam-packaged.zip"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure --prefix=$cur__install", 
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "combine_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "combine_version": {
            "scope": "global", 
            "val": "0.55.0"
          }, 
          "combine_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/menhir": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/combine"
    }, 
