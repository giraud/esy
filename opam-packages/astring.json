{
  "versions": {
    "0.8.3": {
      "version": "0.8.3", 
      "opam": {
        "url": "http://erratique.ch/software/astring/releases/astring-0.8.3.tbz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml pkg/pkg.ml build --pinned false", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "astring_version": {
            "scope": "global", 
            "val": "0.8.3"
          }, 
          "astring_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "astring_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/topkg": "*", 
        "@opam/ocamlbuild": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.01.0"
      }, 
      "name": "@opam/astring"
    }
  }
}