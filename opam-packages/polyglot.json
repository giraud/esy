{
  "versions": {
    "1.0.0": {
      "version": "1.0.0", 
      "opam": {
        "url": "https://github.com/dsheets/polyglot/archive/1.0.0.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "substs lib/META.in", 
          "substs polyglot.version.in", 
          "make lib", 
          "make tool", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "polyglot_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "polyglot_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "polyglot_version": {
            "scope": "global", 
            "val": "1.0.0"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/cmdliner": "*", 
        "@opam/alcotest": "*", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/xmlm": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/base-unix": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.00.0"
      }, 
      "name": "@opam/polyglot"
    }
  }
}