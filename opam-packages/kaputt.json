{
  "versions": {
    "1.1.0": {
      "version": "1.1.0", 
      "opam": {
        "url": "http://kaputt.x9c.fr/distrib/kaputt-1.1.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "sh configure", 
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "kaputt_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "kaputt_version": {
            "scope": "global", 
            "val": "1.1.0"
          }, 
          "kaputt_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/kaputt"
    }, 
    "1.2.0": {
      "version": "1.2.0", 
      "opam": {
        "url": "http://forge.ocamlcore.org/frs/download.php/987/kaputt-1.2.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "sh configure", 
          "make all", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "kaputt_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "kaputt_version": {
            "scope": "global", 
            "val": "1.2.0"
          }, 
          "kaputt_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/kaputt"
    }
  }
}