  "versions": {
    "0.10.0": {
      "version": "0.10.0", 
      "opam": {
        "url": "https://github.com/djs55/ocaml-hvsock/archive/v0.10.0.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml setup.ml -configure", 
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "hvsock_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "hvsock_version": {
            "scope": "global", 
            "val": "0.10.0"
          }, 
          "hvsock_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "@opam/cmdliner": "*", 
        "@opam/logs": "*", 
        "@opam/lwt": ">= 2.4.7", 
        "@opam/cstruct": "*", 
        "@opam/alcotest": ">= 0.4.0", 
        "@opam/mirage-types-lwt": "< 3.0.0", 
        "@opam/ocamlfind": "*", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/mirage-flow": "<=1.1.0", 
        "@opam/fmt": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.01.0"
      }, 
      "name": "@opam/hvsock"
    }, 
