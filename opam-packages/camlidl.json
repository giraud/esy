{
  "versions": {
    "1.05.0": {
      "version": "1.05.0", 
      "opam": {
        "url": "http://caml.inria.fr/pub/old_caml_site/distrib/bazar-ocaml/camlidl-1.05.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "mv config/Makefile.unix config/Makefile", 
          "mv config/Makefile.win32 config/Makefile", 
          "make all", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "camlidl_version": {
            "scope": "global", 
            "val": "1.05.0"
          }, 
          "camlidl_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "camlidl_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/camlidl"
    }
  }
}