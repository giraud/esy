{
  "versions": {
    "1.0.0": {
      "version": "1.0.0", 
      "opam": {
        "url": "https://github.com/xvw/jsoo_router/archive/v1.0.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure --prefix=$cur__install", 
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "jsoo_router_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "jsoo_router_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "jsoo_router_version": {
            "scope": "global", 
            "val": "1.0.0"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/js_of_ocaml": ">= 2.8.1", 
        "@opam/lwt": ">= 2.5.2", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/ocamlfind": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.03.0"
      }, 
      "name": "@opam/jsoo_router"
    }
  }
}