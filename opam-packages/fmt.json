{
  "versions": {
    "0.8.0": {
      "version": "0.8.0", 
      "opam": {
        "url": "http://erratique.ch/software/fmt/releases/fmt-0.8.0.tbz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml pkg/pkg.ml build --pinned false --with-base-unix ${base_unix_installed:-false} --with-cmdliner ${cmdliner_installed:-false}", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "fmt_version": {
            "scope": "global", 
            "val": "0.8.0"
          }, 
          "fmt_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "fmt_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/topkg": ">= 0.7.5", 
        "@opam/ocamlbuild": "*", 
        "@opam/cmdliner": "*", 
        "@opam/result": "*", 
        "substs": "esy-ocaml-test/substs", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/base-unix": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.01.0"
      }, 
      "name": "@opam/fmt"
    }
  }
}