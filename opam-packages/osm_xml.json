{
  "versions": {
    "0.0.1": {
      "version": "0.0.1", 
      "opam": {
        "url": "https://github.com/aluuu/osm_xml/archive/v0.0.1.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure --${ounit_enable:-disable}-tests --prefix=$cur__install", 
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "osm_xml_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "osm_xml_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "osm_xml_version": {
            "scope": "global", 
            "val": "0.0.1"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/core": "*", 
        "@opam/ounit": "*", 
        "@opam/xmlm": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.01.0"
      }, 
      "name": "@opam/osm_xml"
    }
  }
}