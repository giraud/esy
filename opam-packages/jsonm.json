{
  "versions": {
    "0.9.1": {
      "version": "0.9.1", 
      "opam": {
        "url": "http://erratique.ch/software/jsonm/releases/jsonm-0.9.1.tbz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "jsonm_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "jsonm_version": {
            "scope": "global", 
            "val": "0.9.1"
          }, 
          "jsonm_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/uutf": "<= 0.9.4", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 3.12.0"
      }, 
      "name": "@opam/jsonm"
    }, 
    "1.0.0": {
      "version": "1.0.0", 
      "opam": {
        "url": "http://erratique.ch/software/jsonm/releases/jsonm-1.0.0.tbz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml pkg/pkg.ml build --pinned false", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "jsonm_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "jsonm_version": {
            "scope": "global", 
            "val": "1.0.0"
          }, 
          "jsonm_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "@opam/uchar": "*", 
        "@opam/ocamlfind": "*", 
        "@opam/topkg": "*", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/uutf": ">= 1.0.0", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.01.0"
      }, 
      "name": "@opam/jsonm"
    }
  }
}