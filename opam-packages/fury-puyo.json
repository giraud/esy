{
  "versions": {
    "0.5.0": {
      "version": "0.5.0", 
      "opam": {
        "url": "https://forge.ocamlcore.org/frs/download.php/352/furypuyo-0.5.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "substs data_dir.patch.in", 
          "make", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "fury_puyo_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "fury_puyo_version": {
            "scope": "global", 
            "val": "0.5.0"
          }, 
          "fury_puyo_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/conf-sdl-image": "*", 
        "@opam/ocamlsdl": "*", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/conf-sdl-mixer": "*", 
        "@opam/conf-sdl-ttf": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/fury-puyo"
    }
  }
}