{
  "versions": {
    "1.1.0": {
      "version": "1.1.0", 
      "opam": {
        "url": "https://github.com/BinaryAnalysisPlatform/bap/archive/v1.1.0.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure --prefix=$cur__install --enable-api", 
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "bap_api_version": {
            "scope": "global", 
            "val": "1.1.0"
          }, 
          "bap_api_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "bap_api_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/cmdliner": "*", 
        "@opam/bap-std": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/bap-api"
    }, 
