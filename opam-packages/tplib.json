{
  "versions": {
    "1.3.0": {
      "version": "1.3.0", 
      "opam": {
        "url": "https://gforge.inria.fr/frs/download.php/32084/tplib-1.3.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure --prefix $cur__install", 
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "tplib_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "tplib_version": {
            "scope": "global", 
            "val": "1.3.0"
          }, 
          "tplib_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/mlgmp": "*", 
        "@opam/zarith": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 3.12.0"
      }, 
      "name": "@opam/tplib"
    }
  }
}