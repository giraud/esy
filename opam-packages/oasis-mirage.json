{
  "versions": {
    "0.3.0": {
      "version": "0.3.0", 
      "opam": {
        "url": "https://github.com/avsm/oasis/archive/avsm+mirage-xen.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "oasis_mirage_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "oasis_mirage_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "oasis_mirage_version": {
            "scope": "global", 
            "val": "0.3.0"
          }
        }
      }, 
      "dependencies": {
        "@opam/fileutils": "*", 
        "@opam/ocamlfind": "*", 
        "@opam/ocamlmod": "*", 
        "@opam/ocaml-data-notation": "*", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlify": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/oasis-mirage"
    }, 
