{
  "versions": {
    "0.1.0": {
      "version": "0.1.0", 
      "opam": {
        "url": "https://github.com/gildor478/opam-build-revdeps/releases/download/0.1.0/opam-build-revdeps-0.1.0.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "opam_build_revdeps_version": {
            "scope": "global", 
            "val": "0.1.0"
          }, 
          "opam_build_revdeps_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "opam_build_revdeps_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "@opam/fileutils": ">= 0.5.1", 
        "@opam/cmdliner": ">= 0.9.0", 
        "@opam/re": ">= 1.7.0", 
        "@opam/ocamlfind": ">= 1.3.1", 
        "@opam/uuidm": ">= 0.9.6", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/opam-lib": "= 1.2.2", 
        "@opam/jingoo": ">= 1.2.0", 
        "@opam/calendar": ">= 2.03.0", 
        "@opam/ocamlify": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 3.12.1"
      }, 
      "name": "@opam/opam-build-revdeps"
    }
  }
}