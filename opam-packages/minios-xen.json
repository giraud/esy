{
  "versions": {
    "0.9.0": {
      "version": "0.9.0", 
      "opam": {
        "url": "https://github.com/talex5/mini-os/archive/v0.9.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "sh -ex ./build-opam.sh", 
          "make install LIBDIR=$cur__install/lib INCLUDEDIR=$cur__install/include", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "minios_xen_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "minios_xen_version": {
            "scope": "global", 
            "val": "0.9.0"
          }, 
          "minios_xen_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/conf-perl": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/minios-xen"
    }, 
