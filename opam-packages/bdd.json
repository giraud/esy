{
  "versions": {
    "0.3.0": {
      "version": "0.3.0", 
      "opam": {
        "url": "https://www.lri.fr/~filliatr/ftp/ocaml/bdd/bdd-0.3.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "obuild configure", 
          "obuild build", 
          "obuild install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "bdd_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "bdd_version": {
            "scope": "global", 
            "val": "0.3.0"
          }, 
          "bdd_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/obuild": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/bdd"
    }
  }
}