{
  "versions": {
    "1.4.0": {
      "version": "1.4.0", 
      "opam": {
        "url": "https://forge.ocamlcore.org/frs/download.php/1043/bolt-1.4.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure -ocaml-prefix $cur__install -ocamlfind $cur__bin/ocamlfind", 
          "make all", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "bolt_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "bolt_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "bolt_version": {
            "scope": "global", 
            "val": "1.4.0"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/camlp4": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/bolt"
    }, 
