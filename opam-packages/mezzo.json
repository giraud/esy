{
  "versions": {
    "0.0.m8": {
      "version": "0.0.m8", 
      "opam": {
        "url": "https://github.com/protz/mezzo/archive/m8.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure", 
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "mezzo_version": {
            "scope": "global", 
            "val": "0.0.m8"
          }, 
          "mezzo_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "mezzo_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/functory": "*", 
        "@opam/fix": "*", 
        "@opam/yojson": "*", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/menhir": "*", 
        "@opam/pprint": "*", 
        "@opam/ulex": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.00.1"
      }, 
      "name": "@opam/mezzo"
    }
  }
}