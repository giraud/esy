{
  "versions": {
    "0.8.1": {
      "version": "0.8.1", 
      "opam": {
        "url": "http://erratique.ch/software/topkg/releases/topkg-0.8.1.tbz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml pkg/pkg.ml build --pkg-name topkg --pinned false", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "topkg_version": {
            "scope": "global", 
            "val": "0.8.1"
          }, 
          "topkg_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "topkg_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": ">= 1.6.1", 
        "@opam/ocamlbuild": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/result": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.01.0"
      }, 
      "name": "@opam/topkg"
    }
  }
}