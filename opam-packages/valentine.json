{
  "versions": {
    "1.0.0": {
      "version": "1.0.0", 
      "opam": {
        "url": "https://github.com/fxfactorial/valentine/archive/v1.0.0.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "oasis setup", 
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "valentine_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "valentine_version": {
            "scope": "global", 
            "val": "1.0.0"
          }, 
          "valentine_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "@opam/cmdliner": "*", 
        "@opam/oasis": ">= 0.4.0", 
        "@opam/ocamlfind": "*", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/markup": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.02.0"
      }, 
      "name": "@opam/valentine"
    }
  }
}