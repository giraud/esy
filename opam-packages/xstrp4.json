{
  "versions": {
    "1.8.0": {
      "version": "1.8.0", 
      "opam": {
        "url": "http://download.camlcity.org/download/xstrp4-1.8.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure", 
          "make all", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "xstrp4_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "xstrp4_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "xstrp4_version": {
            "scope": "global", 
            "val": "1.8.0"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/camlp4": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/xstrp4"
    }
  }
}