{
  "versions": {
    "0.0.0": {
      "version": "0.0.0", 
      "opam": {
        "url": null
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "mkdir -p $cur__lib/findlib.conf.d/", 
          "cp metaocaml.native.conf $cur__lib/findlib.conf.d/metaocaml.conf", 
          "cp metaocaml.conf $cur__lib/findlib.conf.d/metaocaml.conf", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "base_metaocaml_ocamlfind_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "base_metaocaml_ocamlfind_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "base_metaocaml_ocamlfind_version": {
            "scope": "global", 
            "val": "0.0.0"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/base-metaocaml-ocamlfind"
    }
  }
}