{
  "versions": {
    "2.0.0": {
      "version": "2.0.0", 
      "opam": {
        "url": "http://forge.ocamlcore.org/frs/download.php/1411/pgocaml-2.0.tgz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure --prefix $cur__install --docdir $cur__doc/pgocaml", 
          "make", 
          "make doc", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "pgocaml_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "pgocaml_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "pgocaml_version": {
            "scope": "global", 
            "val": "2.0.0"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/camlp4": "*", 
        "@opam/csv": "*", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/calendar": "*", 
        "@opam/pcre": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.00.0"
      }, 
      "name": "@opam/pgocaml"
    }, 
