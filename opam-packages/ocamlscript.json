{
  "versions": {
    "2.0.4": {
      "version": "2.0.4", 
      "opam": {
        "url": "https://github.com/mjambon/ocamlscript/archive/v2.0.4.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "ocamlscript_version": {
            "scope": "global", 
            "val": "2.0.4"
          }, 
          "ocamlscript_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "ocamlscript_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/camlp4": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/ocamlscript"
    }, 
