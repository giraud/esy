{
  "versions": {
    "1.0.0": {
      "version": "1.0.0", 
      "opam": {
        "url": null
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "sh -c cc test.c -lppl_c -lppl != freebsd & os != openbsd } cc test.c -I/usr/local/include -L/usr/local/lib -lppl_c -lppl = freebsd | os = openbsd }", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "conf_ppl_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "conf_ppl_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "conf_ppl_version": {
            "scope": "global", 
            "val": "1.0.0"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/conf-ppl"
    }
  }
}