{
  "versions": {
    "0.9.8": {
      "version": "0.9.8", 
      "opam": {
        "url": "https://github.com/mjambon/mjambon2016/raw/master/json-static-0.9.8.tar.bz2"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make", 
          "make install PREFIX=$cur__install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "json_static_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "json_static_version": {
            "scope": "global", 
            "val": "0.9.8"
          }, 
          "json_static_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml-test/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/json-wheel": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin", 
        "@opam/camlp4": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/json-static"
    }
  }
}