{
  "versions": {
    "1.0.0": {
      "version": "1.0.0", 
      "opam": {
        "url": "https://github.com/OCamlPro/ocaml-perf/archive/0.2.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "oasis setup", 
          "make", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "perf_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "perf_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "perf_version": {
            "scope": "global", 
            "val": "1.0.0"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/oasis": "*", 
        "@opam/ppx_deriving": "*", 
        "@opam/ocplib-endian": "*", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/oclock": "*", 
        "@opam/sexplib": "< 113.01.00", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/perf"
    }
  }
}