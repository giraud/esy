{
  "versions": {
    "0.6.0": {
      "version": "0.6.0", 
      "opam": {
        "url": "https://github.com/alavrik/piqi/archive/v0.6.0.tar.gz"
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure --prefix $cur__install", 
          "make", 
          "make ocaml", 
          "make install", 
          "make ocaml-install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "piqi_version": {
            "scope": "global", 
            "val": "0.6.0"
          }, 
          "piqi_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "piqi_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/xmlm": "*", 
        "substs": "esy-ocaml-test/substs", 
        "@opam/camlp4": "< 4.02.0", 
        "@opam/easy-format": "*", 
        "@opam/ulex": "*", 
        "opam-installer-bin": "andreypopp/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/piqi"
    }, 
